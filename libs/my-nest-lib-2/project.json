{
  "name": "my-nest-lib-2",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/my-nest-lib-2/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/my-nest-lib-2",
        "tsConfig": "libs/my-nest-lib-2/tsconfig.lib.json",
        "packageJson": "libs/my-nest-lib-2/package.json",
        "main": "libs/my-nest-lib-2/src/index.ts",
        "assets": ["libs/my-nest-lib-2/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/my-nest-lib-2/jest.config.ts"
      }
    }
  },
  "tags": []
}
